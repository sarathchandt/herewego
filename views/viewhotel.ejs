	<script>
		function gotogoa(t) {

			if (t == "goa") {

				let x = document.querySelectorAll(`.${t}`);
				for (let i = 0; i < x.length; i++) {
					x[i].style.display = "block"
				}

				document.querySelector("#all").style.backgroundColor = "white";
				document.querySelector("#all").style.color = "black";

				document.querySelector("#goa").style.backgroundColor = 'black';
				document.querySelector("#goa").style.color = "white";

				document.querySelector("#hyderabad").style.backgroundColor = "white";
				document.querySelector("#hyderabad").style.color = "black";

				let y = document.querySelectorAll('.hyderabad');

				for (let i = 0; i < y.length; i++) {
					y[i].style.display = "none"
				}

			} else if (t == "hyderabad") {
				let x = document.querySelectorAll(`.${t}`);
				for (let i = 0; i < x.length; i++) {
					x[i].style.display = "block"


					document.querySelector("#all").style.backgroundColor = "white";
					document.querySelector("#all").style.color = "black";

					document.querySelector("#goa").style.backgroundColor = 'white';
					document.querySelector("#goa").style.color = "black";

					document.querySelector("#hyderabad").style.backgroundColor = "black";
					document.querySelector("#hyderabad").style.color = "white";

				}
				let y = document.querySelectorAll('.goa');

				for (let i = 0; i < y.length; i++) {
					y[i].style.display = "none"
				}

			} else {
				let x = document.querySelectorAll('.goa');
				for (let i = 0; i < x.length; i++) {
					x[i].style.display = "block"
				}
				document.querySelector("#all").style.backgroundColor = "black";
				document.querySelector("#all").style.color = "white";

				document.querySelector("#goa").style.backgroundColor = 'white';
				document.querySelector("#goa").style.color = "black";

				document.querySelector("#hyderabad").style.backgroundColor = "white";
				document.querySelector("#hyderabad").style.color = "black";

				let y = document.querySelectorAll(".hyderabad");
				for (let i = 0; i < y.length; i++) {
					y[i].style.display = "block"
				}
			}
		}
	</script>

